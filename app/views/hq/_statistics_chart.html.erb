<script src="/assets/highCharts/highcharts.js"></script>
<script src="/assets/highCharts/exporting.js"></script>

<div id="chart-container" style="width: 98%; height: 300px; margin: 0 auto"></div>
<script type="text/javascript">
    function loadChart(title){
          $.getJSON('/dashbord_data',function(data){
                  loadTable(data);
                  $(function () {
                    $('#chart-container').highcharts({
                        title: {
                            text: (title? title : 'National statistics'),
                            x: -20 //center
                        },
                        subtitle: {
                            text: '',
                            x: -20
                        },
                        xAxis: {
                            categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun',
                                'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
                        },
                        yAxis: {
                            allowDecimals: false,
                            title: {
                                text: 'Number of cases'
                            },
                            plotLines: [{
                                value: 0,
                                width: 1,
                                color: '#808080'
                            }]
                        },
                        credits: {
                          enabled: false
                        },
                        tooltip: {
                            valueSuffix: ''
                        },
                        exporting: {
                          enabled: false
                        },
                        legend: {
                          enabled: false
                        },
                        series: [{
                            name: 'Newly Received (HQ)',
                            data: data["year_registered"]
                        }, {
                            name: 'Approved (HQ)',
                             data: data["year_approved"]
                        }, {
                            name: 'Printed and Dispatched',
                             data: data["year_printed"]
                        }]
                    });
                });  
            });

    }
    $(document).ready(function(){
         loadChart();
         setInterval(function(){
                 loadChart();
         },5000)
        
    });
</script>

